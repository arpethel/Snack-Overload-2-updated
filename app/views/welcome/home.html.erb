<% if session[:user_id] %>
    <%= render 'users/show' %>
<% else %>
  <div>
    <%= link_to(image_tag('Continue_with_FB.svg'), '/auth/facebook') %>
  </div>
  <div class="row">
    <div class="col-sm-4"></div>
    <div class="col-sm-4" style="margin-top:3rem">
      <div>
        <h3> <a href="<%= signup_url %>">Or Create Account With Email</a></h3>
      </div>
    </div>
    <div class="col-sm-4"></div>
  </div>
<% end %>
